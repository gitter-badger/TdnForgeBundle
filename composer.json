{
    "name": "tdn/sfprojectgeneratorbundle",
    "description": "Project generator for Symfony 2.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Passapera",
            "email": "vpassapera@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "friendsofsymfony/rest-bundle": "1.4.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "jms/serializer-bundle": "0.13.*",
        "nelmio/cors-bundle": "*@stable",
        "nelmio/api-doc-bundle": "~2.7",
        "sensio/generator-bundle": "*@stable",
        "doctrine/doctrine-bundle": "*@stable",
        "tdn/php-types": "dev-master",
        "fzaninotto/faker": "1.4.0"
    },
    "require-dev": {
        "raulfraile/ladybug": "dev-master",
        "phpunit/phpunit": "*@stable",
        "mockery/mockery": "dev-master",
        "doctrine/instantiator": "~1.0,>=1.0.2",
        "squizlabs/php_codesniffer": "*@stable"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Tdn\\SfProjectGeneratorBundle\\": ""
        }
    }
}
